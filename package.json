{
  "version": "0.0.1",
  "name": "my-project",
  "author": "Itti Architecture Team",
  "scripts": {
    "dev": "APP_ENV=local webpack-cli serve",
    "build": "rm -rf dist/ && APP_ENV=stg webpack-cli build",
    "start": "cd dist && PORT=3003 npx serve",
    "lint": "eslint './**/**/*.{ts,tsx,js}' --no-error-on-unmatched-pattern",
    "lint-fix": "eslint './**/**/*.{ts,tsx,js}' --no-error-on-unmatched-pattern --fix",
    "prettier": "prettier './**/**/*.{ts,tsx,js}' --no-error-on-unmatched-pattern --write",
    "test": "jest --passWithNoTests",
    "test:debug": "yarn test:unit --runInBand --detectLeaks",
    "test:unit": "jest --testMatch='<rootDir>/src/**/__tests__/**/*.spec.{tsx,ts}' coverageDirectory='coverage/unit'",
    "test:integration": "jest --testMatch='<rootDir>/src/**/__tests__/**/*.ispec.{tsx,ts}' coverageDirectory='coverage/integration'",
    "prepare": "husky install",
    "release": "standard-version"
  },
